{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs18.x"
    }
  }
}